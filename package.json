{
  "name": "wealthhustle-blog",
  "version": "1.0.0",
  "description": "Static blog for wealthhustle.blog built with Gatsby",
  "author": "Echo Reader",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build && npm run generate-sitemap",
    "preview": "gatsby serve",
    "start": "npm run dev",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "generate-sitemap": "npm run generate-sitemap-post && npm run generate-sitemap-page",
    "generate-sitemap-post": "node scripts/generate-sitemap-post.js",
    "generate-sitemap-page": "node scripts/generate-sitemap-page.js"
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^2.3.0",
    "gatsby": "^5.14.6",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-mdx": "^5.15.0",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/echoreader/wealthhustle.blog"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
